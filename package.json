{
  "name": "vcare-pos-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix --report-unused-disable-directives",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "pm2:serve": "pm2 serve dist 3001 --spa --name vcare-pos-frontend",
    "pm2:start:config": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop vcare-pos-frontend",
    "pm2:restart": "pm2 restart vcare-pos-frontend",
    "pm2:status": "pm2 status vcare-pos-frontend",
    "pm2:logs": "pm2 logs vcare-pos-frontend",
    "pm2:delete": "pm2 delete vcare-pos-frontend",
    "start:prod": "npm run build && npm run pm2:serve"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "antd": "^5.12.8",
    "dayjs": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "node-cache": "^5.1.2",
    "pm2": "^5.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-image-crop": "^11.0.10",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.3.0",
    "sass": "^1.69.5",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-eslint": "^1.8.1"
  }
}
